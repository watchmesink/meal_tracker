# Meal Tracker Environment Variables
# Copy this file to .env and replace the placeholder values with your actual API keys

# Gemini AI API Key (required for AI-powered nutrition analysis)
# Get your API key from: https://aistudio.google.com/apikey
GEMINI_API_KEY=your_gemini_api_key_here

# Google Cloud Speech API credentials (optional - for voice input)
GOOGLE_CLOUD_PROJECT_ID=your_project_id_here
GOOGLE_APPLICATION_CREDENTIALS=path/to/your/service-account-key.json

# Server Configuration
PORT=3000
NODE_ENV=development
